{
  "name": "gnis-enterprise-infrastructure",
  "version": "1.0.0",
  "description": "Geographic Names Information System - Enterprise-Grade Infrastructure",
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "test": "jest",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "playwright test",
    "validate-schema": "ajv validate -s gnis/core/geo-object.json -d",
    "pipeline:dev": "ts-node gnis/ingestion/pipelines/ingestion-workflow.ts",
    "deploy:github": "next build && next export && gh-pages -d out",
    "deploy:azure": "az staticwebapp deploy",
    "deploy:gcp": "gcloud app deploy",
    "deploy:all": "npm run deploy:github && npm run deploy:azure && npm run deploy:gcp",
    "security-scan": "npm audit && snyk test",
    "type-check": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coloredinnovator-ai/copilot-cli.git"
  },
  "keywords": [
    "gis",
    "geospatial",
    "geographic",
    "enterprise",
    "multi-cloud",
    "truth-governed"
  ],
  "author": "Enterprise Architecture Team",
  "license": "SEE LICENSE IN LICENSE.md",
  "bugs": {
    "url": "https://github.com/coloredinnovator-ai/copilot-cli/issues"
  },
  "homepage": "https://github.com/coloredinnovator-ai/copilot-cli#readme",
  "dependencies": {
    "next": "^14.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-leaflet": "^4.2.1",
    "leaflet": "^1.9.4",
    "axios": "^1.6.2",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "uuid": "^9.0.1",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.42",
    "@types/react-dom": "^18.2.17",
    "@types/leaflet": "^1.9.8",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "eslint-config-next": "^14.0.0",
    "typescript": "^5.3.2",
    "jest": "^29.7.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/jest-dom": "^6.1.5",
    "@playwright/test": "^1.40.0",
    "ts-node": "^10.9.1",
    "gh-pages": "^6.1.0",
    "snyk": "^1.1266.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
